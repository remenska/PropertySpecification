<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="Sample Multi-page Editor"
            extensions="mpe"
            icon="icons/sample.gif"
            contributorClass="info.remenska.mysecondplugin.editors.MultiPageEditorContributor"
            class="info.remenska.mysecondplugin.editors.MultiPageEditor"
            id="info.remenska.mysecondplugin.editors.MultiPageEditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="Sample Wizards"
            id="info.remenska.mySecondPlugin">
      </category>
      <wizard
            name="Multi-page Editor file"
            icon="icons/sample.gif"
            category="info.remenska.mySecondPlugin"
            class="info.remenska.mysecondplugin.wizards.SampleNewWizard"
            id="info.remenska.mysecondplugin.wizards.SampleNewWizard">
      </wizard>
   </extension>

</plugin>
